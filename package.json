  "bugs": {
    "url": "https://github.com/Hoffalypse/earthship-packaging-dev/issues"
  },
  "homepage": "https://github.com/Hoffalypse/earthship-packaging-dev#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.9.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
